{
  "name": "synt",
  "version": "0.3.4",
  "description": "Similar code analysis.",
  "author": "Brent Lintner <brent.lintner@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/brentlintner/synt.git"
  },
  "engines": {
    "node": ">= 0.12",
    "npm": ">= 2.0.0"
  },
  "keywords": [
    "duplicate",
    "similar",
    "dissimilar",
    "analysis",
    "compare",
    "comparison",
    "javascript",
    "coffeescript",
    "ruby",
    "haskell"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brentlintner/synt/issues"
  },
  "homepage": "https://github.com/brentlintner/synt",
  "bin": {
    "synt": "bin/cli"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "bin/compile",
    "compile-dev": "bin/compile-dev",
    "compile-hs": "bin/compile-hs",
    "bundle-rubygems": "bin/bundle-rb-gems",
    "dev": "bin/dev",
    "dev-setup": "bin/dev-setup",
    "lint-cov": "bin/lint-cov",
    "postinstall": "npm run compile && ((bin/bundle-rb-gems && bin/compile-hs) || true)",
    "test": "bin/test && bin/test-ruby && bin/test-haskell",
    "test-cov": "bin/test-cov",
    "test-ci-build": "bin/test-ci-build",
    "test-ci-build-all-commits": "bin/test-ci-build-all-commits",
    "vile": "vile p -usi --nodecorations"
  },
  "dependencies": {
    "coffee-script": "1.12.5",
    "commander": "2.9.0",
    "esprima": "3.1.3",
    "lodash": "4.17.4",
    "minilog": "3.1.0",
    "nconf": "0.8.4",
    "parse-latin": "3.2.0",
    "shelljs": "0.7.7"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coffeelint": "1.16.0",
    "coveralls": "2.13.0",
    "istanbul": "0.4.5",
    "mimus": "0.2.5",
    "mocha": "3.2.0",
    "npm-check-updates": "^2.10.5",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "wrench": "1.5.9",
    "vile": ">=0.13.0",
    "vile-coffeelint": ">=0.1.14",
    "vile-comment": ">=0.1.17",
    "vile-coverage": ">=0.2.14",
    "vile-depcheck": ">=0.1.12",
    "vile-eclint": ">=0.1.13",
    "vile-git": ">=0.5.1",
    "vile-hlint": ">=0.2.1",
    "vile-language": ">=0.1.12",
    "vile-ncu": ">=0.2.1",
    "vile-nsp": ">=0.2.13",
    "vile-rubocop": ">=0.2.1",
    "vile-rubycritic": ">=0.2.2",
    "vile-stat": ">=0.0.14"
  }
}
